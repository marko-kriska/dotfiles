alt + w
    	firefox

alt + Return
	kitty

alt + shift + Return
	pcmanfm

alt + p 
	rofi -show drun

alt + Print
	flameshot gui

alt + ctrl + p
	rofi-pass	

alt + ctrl + r
	rofi-beats-linux

alt + q
	bspc node -c

alt + m
	bspc desktop -l next

alt + {t,s,f}
	bspc node -t {tiled,floating,fullscreen}

alt + {_,shift + }{h,j,k,l}
	bspc node -{f,s} {west,south,north,east}

alt + {_,shift + }{1-9,0}
	bspc {desktop -f,node -d} '^{1-9,10}'

alt + shift + r
	bspc wm -r

alt + shift + q
	bspc quit

alt + Escape
	pkill -USR1 -x sxhkd
