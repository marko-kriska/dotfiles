alt + w
	firefox

alt + Return
	kitty

alt + shift + Return
	nautilus

alt + p 
	rofi -show drun

alt + Print
	scrot -s -e 'mv $f ~/Downloads'

alt + ctrl + p
	rofi-pass	

alt + ctrl + r
	rofi-beats-linux

alt + ctrl + m
	kitty --class pulsemixer,pulsemixer -e pulsemixer

alt + q
	bspc node -c

alt + m
	bspc desktop -l next

alt + {t,s,f}
	bspc node -t {tiled,floating,fullscreen}

alt + {_,shift + }{h,j,k,l}
	bspc node -{f,s} {west,south,north,east}

alt + {_,shift + }{1-9,0}
	bspc {desktop -f,node -d} '^{1-9,10}'

alt + Tab
	bspc desktop -f last.local

alt + F1
	monitor_1.sh

alt + F2
	monitor_2.sh 

alt + period 
	bspc monitor -f next

alt + comma 
	bspc monitor -f prev

alt + ctrl + {h,j,k,l}
	bspc node -p {west,south,north,east}

alt + ctrl + {1-9}
	bspc node -o 0.{1-9}

alt + ctrl + space
	bspc node -p cancel

alt + shift + r
	bspc wm -r

alt + shift + q
	bspc quit

alt + Escape
	pkill -USR1 -x sxhkd

alt + F12
	xsecurelock -- systemctl suspend -i

XF86AudioRaiseVolume  
	amixer -q set Master 5%+ unmute
XF86AudioLowerVolume    
	amixer -q set Master 5%- unmute
XF86AudioMute
	amixer -q set Master toggle
XF86AudioMicMute
	amixer set Capture toggle
XF86MonBrightnessUp
	brightnessctl set +10
XF86MonBrightnessDown
	brightnessctl set 10-
